{
  "name": "@resourge/schema",
  "version": "1.9.0",
  "main": "./dist/main.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/umd/schema.production.min.js",
  "private": false,
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js",
    "./phoneNumbers": "./dist/phoneNumbers/index.js",
    "./postalCodes": "./dist/postalCodes/index.js",
    "./dist/phoneNumbers": "./dist/phoneNumbers/index.js",
    "./dist/postalCodes": "./dist/postalCodes/index.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "phoneNumbers": [
        "./dist/phoneNumbers/index.js"
      ],
      "postalCodes": [
        "./dist/postalCodes/index.js"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "javascript",
    "schema",
    "validation",
    "validations",
    "javascript validations",
    "schema benchmark",
    "validation",
    "typescript",
    "typescript validation"
  ],
  "files": [
    "dist"
  ],
  "author": "resourge",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/resourge/schema.git"
  },
  "scripts": {
    "bench": "npm --prefix ./benchmark run benchmark",
    "bench:heavy": "npm --prefix ./benchmark run heavy:benchmark",
    "example": "npm --prefix ./src/examples/form run dev",
    "commit": "git pull && git add . && git-cz",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:prod": "cross-env NODE_ENV=production npm run lint",
    "generate_postal_codes": "node ./scripts/codPostal/GenerateCountryPostalCodes.js",
    "fix-types": "node ./scripts/FixTypes.js",
    "remove-postal-code": "node ./scripts/RemovePostalCodeFromIndex.js",
    "remove-phone-number": "node ./scripts/RemovePhoneNumberFromIndex.js",
    "dev": "vite",
    "build": "rollup -c && npm run fix-types && npm run remove-postal-code && npm run remove-phone-number",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^14.1.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@vitejs/plugin-react": "^1.3.0",
    "babel-plugin-dev-expression": "^0.2.3",
    "c8": "^7.11.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-flowtype": "^5.10.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-testing-library": "3.9",
    "jsdom": "^19.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rollup": "^2.75.6",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^19.0.3",
    "typescript": "^4.7.4",
    "vite": "^2.9.12",
    "vite-plugin-checker": "^0.4.6",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.15.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}

{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "experimentalDecorators": true,
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "removeComments": false,
    "baseUrl": "./",
    "paths": {
      "src": [
        "./src"
      ]
    },
    "emitDeclarationOnly": false,
    "incremental": true,
    "tsBuildInfoFile": "/Users/joseoliveira/Desktop/Projects/packages/schema/node_modules/vitest/dist/chunks/tsconfig.tmp.tsbuildinfo"
  },
  "include": [
    "./**/src/**/*",
    ".eslintrc.cjs",
    "rollup.config.js"
  ],
  "exclude": [
    "./**/dist/**/*",
    "dist_old",
    "./src/examples",
    "./node_modules/**/*"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}